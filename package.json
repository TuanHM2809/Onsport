{
  "name": "on_sports",
  "version": "1.0.0",
  "description": "on_sports: Express + nuxtjs",
  "author": "Nghia Than <nghiait0111@gmail.com>",
  "nuxt": {
    "host": "0.0.0.0",
    "port": "7079"
  },
  "private": true,
  "config": {
    "nuxt": {
      "host": "127.0.0.1",
      "port": "8000"
    }
  },
  "scripts": {
    "test": "exit 0",
    "dev": "backpack dev",
    "build-test": "cross-env NODE_ENV=test backpack dev",
    "build": "cross-env NODE_ENV=production nuxt build && cross-env NODE_ENV=production backpack build",
    "start": "cross-env NODE_ENV=production node --max_old_space_size=8192 build/main.js",
    "es": "NODE_ENV=production node mysql2es.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "migrate": "cd server && sequelize db:migrate",
    "seed": "cd server && sequelize db:seed:all",
    "crawl": "cd crawl && node crawling_top11_standings.js && node crawling_top11_matches.js && node crawling_top11_match_events_and_stats.js",
    "stand": "cd crawl && node crawling_top11_standings.js",
    "match": "cd crawl && node crawling_top11_matches.js",
    "event": "cd crawl && node crawling_top11_match_events_and_stats.js",
    "miracle": "npm run migrate && npm run seed && npm run crawl",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
